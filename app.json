{
  "expo": {
    "name": "Liceul Tehnologic Telciu",
    "slug": "liceultehnologictelciu",
    "version": "1.0.1",
    "orientation": "default",
    "icon": "./assets/logotelciu.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logotelciu.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "liceultehnologictelciu",
      "infoPlist": {
        "NSCameraUsageDescription": "Aceasta aplicatie foloseste camerea ca sa poti sa adaugi documente. "
      },
      "buildNumber": "18"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logotelciu.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dobricean_ioan.telciu",
      "versionCode": 7
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/logotelciu.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8ec77fcb-75af-47c5-a582-7b2f2bf53ddb"
      }
    }
  }
}
