{
  "expo": {
    "name": "mySGMVT",
    "slug": "mysgmvt",
    "version": "1.0.1",
    "orientation": "default",
    "icon": "./assets/sgmvt.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/sgmvt.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mysgmvt",
      "infoPlist": {
        "NSCameraUsageDescription": "Aceasta aplicatie foloseste camerea ca sa poti sa adaugi documente. "
      },
      "buildNumber": "18"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/sgmvt.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dobricean_ioan.mysgmvt",
      "versionCode": 16
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/sgmvt.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1f01028b-7e90-4858-b0f1-374d18b72050"
      }
    }
  }
}
